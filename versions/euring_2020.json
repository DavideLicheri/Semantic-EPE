  "validation_rules": [
    {
      "field_name": "species_code",
      "rule_type": "format",
      "rule_expression": "value.isdigit() and len(value) == 5",
      "error_message": "Species code must be exactly 5 digits"
    },
    {
      "field_name": "ring_number",
      "rule_type": "format",
      "rule_expression": "len(value) == 8 and value[:3].isalpha() and value[3:].isdigit() and value[:3].isupper()",
      "error_message": "Ring number must be 3 uppercase letters followed by 5 digits"
    },
    {
      "field_name": "age_code",
      "rule_type": "range",
      "rule_expression": "value.isdigit() and len(value) == 1 and int(value) >= 1",
      "error_message": "Age code must be a single digit from 1-9"
    },
    {
      "field_name": "sex_code",
      "rule_type": "range",
      "rule_expression": "value.isdigit() and len(value) == 1 and int(value) >= 1",
      "error_message": "Sex code must be a single digit from 1-9"
    },
    {
      "field_name": "date_code",
      "rule_type": "format",
      "rule_expression": "len(value) == 8 and value.isdigit() and 1 <= int(value[6:8]) <= 31 and 1 <= int(value[4:6]) <= 12 and int(value[:4]) >= 1900",
      "error_message": "Date must be 8 digits in valid YYYYMMDD format"
    },
    {
      "field_name": "time_code",
      "rule_type": "format",
      "rule_expression": "len(value) == 4 and value.isdigit() and int(value[:2]) <= 23 and int(value[2:]) <= 59",
      "error_message": "Time must be 4 digits in valid HHMM format (00:00-23:59)"
    },
    {
      "field_name": "latitude_decimal",
      "rule_type": "range",
      "rule_expression": "-90 <= float(value) <= 90",
      "error_message": "Latitude must be between -90 and 90 degrees"
    },
    {
      "field_name": "longitude_decimal",
      "rule_type": "range",
      "rule_expression": "-180 <= float(value) <= 180",
      "error_message": "Longitude must be between -180 and 180 degrees"
    },
    {
      "field_name": "metal_ring_info",
      "rule_type": "range",
      "rule_expression": "value.isdigit() and len(value) == 1",
      "error_message": "Metal ring info must be a single digit"
    },
    {
      "field_name": "other_marks_info",
      "rule_type": "format",
      "rule_expression": "len(value) == 5 and value.isdigit()",
      "error_message": "Other marks info must be exactly 5 digits"
    },
    {
      "field_name": "method_code",
      "rule_type": "format",
      "rule_expression": "len(value) == 2 and value.isdigit()",
      "error_message": "Method code must be exactly 2 digits"
    },
    {
      "field_name": "accuracy_code",
      "rule_type": "format",
      "rule_expression": "len(value) == 2 and value.isdigit()",
      "error_message": "Accuracy code must be exactly 2 digits"
    },
    {
      "field_name": "wing_length",
      "rule_type": "range",
      "rule_expression": "float(value) > 0 and float(value) < 1000",
      "error_message": "Wing length must be a positive decimal less than 1000mm"
    },
    {
      "field_name": "weight",
      "rule_type": "range",
      "rule_expression": "float(value) > 0 and float(value) < 10000",
      "error_message": "Weight must be a positive decimal less than 10000g"
    }
  ],
  "format_specification": {
    "total_length": 102,
    "field_separator": "|",
    "encoding": "utf-8",
    "validation_pattern": "^[0-9]{5}\\|[A-Z]{3}[0-9]{5}\\|[0-9]\\|[0-9]{5}\\|[1-9]\\|[1-9]\\|[0-9]{8}\\|[0-9]{4}\\|[+-]?[0-9]+\\.[0-9]+\\|[+-]?[0-9]+\\.[0-9]+\\|[0-9]\\|[0-9]{2}\\|[0-9]{2}\\|[0-9]\\|[0-9]\\|[0-9]+\\.[0-9]\\|[0-9]+\\.[0-9]\\|[0-9]\\|[0-9]\\|[0-9]\\|[0-9]\\|[0-9]$"
  },
