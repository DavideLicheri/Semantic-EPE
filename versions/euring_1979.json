  "validation_rules": [
    {
      "field_name": "species_code",
      "rule_type": "format",
      "rule_expression": "value.isdigit() and len(value) == 5",
      "error_message": "Species code must be exactly 5 digits"
    },
    {
      "field_name": "scheme_country",
      "rule_type": "format",
      "rule_expression": "value.isalpha() and len(value) == 2 and value.isupper()",
      "error_message": "Scheme country must be 2 uppercase letters"
    },
    {
      "field_name": "ring_number",
      "rule_type": "format",
      "rule_expression": "len(value) == 7 and value[0].isalpha() and value[1:7].isdigit()",
      "error_message": "Ring number must be 1 letter followed by 6 digits"
    },
    {
      "field_name": "age_code",
      "rule_type": "range",
      "rule_expression": "value.isdigit() and len(value) == 1",
      "error_message": "Age code must be a single digit"
    },
    {
      "field_name": "sex_code",
      "rule_type": "range",
      "rule_expression": "value.isdigit() and len(value) == 1",
      "error_message": "Sex code must be a single digit"
    },
    {
      "field_name": "date_first",
      "rule_type": "format",
      "rule_expression": "len(value) == 6 and value.isdigit() and 1 <= int(value[:2]) <= 31 and 1 <= int(value[2:4]) <= 12",
      "error_message": "First date must be 6 digits in valid DDMMYY format"
    },
    {
      "field_name": "date_current",
      "rule_type": "format",
      "rule_expression": "len(value) == 6 and value.isdigit() and 1 <= int(value[:2]) <= 31 and 1 <= int(value[2:4]) <= 12",
      "error_message": "Current date must be 6 digits in valid DDMMYY format"
    },
    {
      "field_name": "latitude_encoded",
      "rule_type": "format",
      "rule_expression": "len(value) == 6 and value[:-1].isdigit() and value[-1] in ['N', 'S'] and 0 <= int(value[:2]) <= 90 and 0 <= int(value[2:4]) <= 59",
      "error_message": "Latitude must be valid degrees (00-90) and minutes (00-59) followed by N or S"
    },
    {
      "field_name": "longitude_encoded",
      "rule_type": "format",
      "rule_expression": "len(value) == 6 and value[:-1].isdigit() and value[-1] in ['E', 'W'] and 0 <= int(value[:3]) <= 180 and 0 <= int(value[3:5]) <= 59",
      "error_message": "Longitude must be valid degrees (000-180) and minutes (00-59) followed by E or W"
    },
    {
      "field_name": "wing_length",
      "rule_type": "format",
      "rule_expression": "len(value) == 3 and value.isdigit()",
      "error_message": "Wing length must be exactly 3 digits"
    },
    {
      "field_name": "weight",
      "rule_type": "format",
      "rule_expression": "len(value) == 4 and value.isdigit()",
      "error_message": "Weight must be exactly 4 digits"
    },
    {
      "field_name": "bill_length",
      "rule_type": "format",
      "rule_expression": "len(value) == 4 and value.isdigit()",
      "error_message": "Bill length must be exactly 4 digits"
    },
    {
      "field_name": "empty_fields_1",
      "rule_type": "exact",
      "rule_expression": "value == '--'",
      "error_message": "Empty fields must be exactly '--'"
    },
